//定义if分支语法
void IfStatement(boolean bComple) :
{
	jjtThis.setLogShow(logshow);
	jjtThis.setTabAndStack(this.g_Systab,this.func_Systab,this.function_Systab,this.sysStack,g_DataDefine);	
}
{
	(
		<IF> <LPAREN> IfConditionExpr() <RPAREN> StatementClause(bComple)
		( 
		 	LOOKAHEAD(<ELSE> <IF>) 
		 	<ELSE> <IF> 
		 	<LPAREN> IfConditionExpr() <RPAREN> StatementClause(bComple) 
		)*
		[ 
		 	LOOKAHEAD(<ELSE>) 
		 	<ELSE> StatementClause(bComple) 
		 ]
	)
}
//if statement
void StatementClause(boolean bCompile):
{
	jjtThis.setLogShow(logshow);
	jjtThis.setTabAndStack(this.g_Systab,this.func_Systab,this.function_Systab,this.sysStack,g_DataDefine);	
}
{
	(
		LOOKAHEAD(<LBRACE> Statement())
		(<LBRACE> (Statement(bCompile))+ <RBRACE>)
		|
		Statement(bCompile)
	)
}
//if ConditionExp
void IfConditionExpr(): 
{
	jjtThis.setLogShow(logshow);
	jjtThis.setTabAndStack(this.g_Systab,this.func_Systab,this.function_Systab,this.sysStack,g_DataDefine);	
}
{
	(
		LOOKAHEAD(Name() (<IN> | <NOTIN>))
		NotInorIn()
		|
		ConditionalExpression()
	)
}
