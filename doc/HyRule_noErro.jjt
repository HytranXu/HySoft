//从OR开始定义，OR优先级最低，优先级1
String ConditionalExpression() :
{
	String strLeftValue = new String();
	String strRightValue = new String();
	Token oper = null;
	jjtThis.setLogShow(logshow);
	jjtThis.setTabAndStack(this.g_Systab,this.func_Systab,this.function_Systab,this.sysStack,g_DataDefine);	
}
{
	(
		strLeftValue = ConditionalAndExpression() 
		( 
			oper = <SC_OR> strRightValue = ConditionalAndExpression() 	 
			{
				jjtThis.addStrRightValue(strRightValue);
				if(oper != null)
					jjtThis.addStrOperator(oper.image);
				oper = null;
			}
		)*
	)
	{	
		jjtThis.setStrLeftValue(strLeftValue);
		
		return "";
	}
}
//AND表达式，优先级2
String ConditionalAndExpression() :
{
	String strLeftValue = new String();
	String strRightValue = new String();
	Token oper = null;	
	jjtThis.setLogShow(logshow);
	jjtThis.setTabAndStack(this.g_Systab,this.func_Systab,this.function_Systab,this.sysStack,g_DataDefine);	
}
{
	strLeftValue = EqualityExpression() 
	(  
		oper = <SC_AND> strRightValue = EqualityExpression() 
		{
			jjtThis.addStrRightValue(strRightValue);
			if(oper != null)
				jjtThis.addStrOperator(oper.image);
			oper = null;
		}
	)*
	{	
		jjtThis.setStrLeftValue(strLeftValue);
		
		return "";
	}
}
