//后++|--单目表达式，优先级9
String PostfixExpression() :
{
	Token operat = null,identi = null,This = null;
	String strValue = new String();

	//[0]数组名 [1]下标名 [2]变量名
	String[] arrayItem = null;
	jjtThis.setLogShow(logshow);
	jjtThis.setTabAndStack(this.g_Systab,this.func_Systab,this.function_Systab,this.sysStack,g_DataDefine);	
	boolean[] bValueSimple = new boolean[1];
}
{	
	(
		LOOKAHEAD(<THIS> <DOT> <IDENTIFIER> (<INCR> | <DECR>))
		(This = <THIS> <DOT> identi = <IDENTIFIER>(operat = <INCR> | operat = <DECR>))
		|
		(This = <THIS> <DOT> identi = <IDENTIFIER>)
		|
		LOOKAHEAD(Name() (<INCR> | <DECR>))
		(strValue = Name() (operat = <INCR> | operat = <DECR>))
		|
		LOOKAHEAD(CallFuncLookAhead())
		strValue = CallFuncStatement()
		{
			jjtThis.setWaitForChild(true);
		}
		|
		LOOKAHEAD(Name() <LBRACKET>)
		arrayItem = ArrayItem()
		|
		strValue = SimpleExpress(bValueSimple)
	)
	{
		jjtThis.setValueNotCalc(bValueSimple[0]);
		if(operat != null)
			jjtThis.setStrOper(operat.image);
		
		if(arrayItem != null)
		{
			//[0]数组名 [1]下标名 [2]变量名 oper下标，left数组名,right变量名
			OperationDef aItem = new OperationDef(arrayItem[1],arrayItem[0],arrayItem[2]);
			jjtThis.setArrayItem(aItem);
		}
		else if(identi != null)
			jjtThis.setStrValue(FuncConst.THIS + FuncConst.DOT + identi.image);
		else
			jjtThis.setStrValue(strValue);
		
		return "";
	}
}
//SimpleExpress
String SimpleExpress(boolean[] bValueSimple) #void:
{
	Token value = null;
	String strValue = new String();
	boolean bChar = false, bStr = false;
}
{
	(
		strValue = Name()
		| 
		value = <TRUE> 
		{
			bValueSimple[0] = true;
		}
		| 
		value = <FALSE>
		{
			bValueSimple[0] = true;
		}
		|
		value = <DECIMAL_LITERAL> 
		{
			bValueSimple[0] = true;
		}
		| 
		value = <INTEGER_LITERAL>
		{
			bValueSimple[0] = true;
		}
		|
		value = <STRING_LITERAL>
		{
			bValueSimple[0] = true;
			bStr = true;
		}
		|
		value = <CHARACTER_LITERAL>
		{
			bValueSimple[0] = true;
			bChar = true;
		}
		|
		value = <FLOATING_POINT_LITERAL>
		{
			bValueSimple[0] = true;
		}
	)
	{
		if(value != null)
		{
			if(bChar)
				return trimeQM(value.image);
			else if(bStr)
				return trimeDQM(value.image);
			else
				return value.image;
		}
		else
			return strValue;
	}
}
