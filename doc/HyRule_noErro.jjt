//ValueLookahead
void ValueLookahead() #void:
{}
{
	<DECIMAL_LITERAL> | <INTEGER_LITERAL> 
	| <FLOATING_POINT_LITERAL> | <CHARACTER_LITERAL> 
	| <STRING_LITERAL> | <TRUE> | <FALSE> | <NULL> 
}
//定义函数值语法
String Value() #void:
{
	Token value = null;
	String strValue = new String();
	boolean bChar = false, bStr = false;
}
{
	(
		LOOKAHEAD(ValueLookahead())
		(
			value = <DECIMAL_LITERAL> | value = <INTEGER_LITERAL> 
			| value = <FLOATING_POINT_LITERAL> | value = <CHARACTER_LITERAL> {bChar = true;}
			| value = <STRING_LITERAL> { bStr = true;}| value = <TRUE> | value = <FALSE> | value = <NULL> 
		)
		|
		strValue = ConditionalExpression()
	)
	{
		if(value != null)
		{
			if(bStr)
				return trimeDQM(value.image);
			else if(bChar)
				return trimeQM(value.image);
			else 
				return value.image;
		}
		else
			return strValue;
	}
}
